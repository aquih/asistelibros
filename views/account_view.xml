<openerp>
    <data>

        <record id="asistelibrost_view_invoice_form" model="ir.ui.view">
            <field name="name">asistelibros.view_invoice_form</field>
            <field name="model">account.invoice</field>
            <field name="inherit_id" ref="account.invoice_form"/>
            <field name="arch" type="xml">
                <field name="move_id" position="after">
                    <field name="criva"/>
                    <field name="valor_constancia"/>
                    <field name="cadi"/>
                    <field name="dua"/>
                </field>
            </field>
        </record>

        <record id="asistelibros_view_invoice_supplier_form" model="ir.ui.view">
            <field name="name">asistelibros.view_invoice_supplier_form</field>
            <field name="model">account.invoice</field>
            <field name="inherit_id" ref="account.invoice_supplier_form"/>
            <field name="arch" type="xml">
                <field name="move_id" position="after">
                    <field name="criva"/>
                    <field name="valor_constancia"/>
                    <field name="cadi"/>
                    <field name="dua"/>
                </field>
            </field>
        </record>

         <record model="ir.ui.view" id="asistelibros_view_account_journal_form">
            <field name="name">asistelibros.view_account_journal_form</field>
            <field name="model">account.journal</field>
            <field name="inherit_id" ref="account.view_account_journal_form"/>
            <field name="arch" type="xml">
                <data>
                    <field name="code" required="0" position="after">
                        <field name="codigo_establecimiento"/>
                    </field>
                </data>
            </field>
        </record>

    </data>
</openerp>
